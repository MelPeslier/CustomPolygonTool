;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CustomPolygonTool"
config/version="1.0"
config/tags=PackedStringArray("addon")
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false
config/icon="res://cpt.png"

[debug]

gdscript/warnings/native_method_override=1

[editor_plugins]

enabled=PackedStringArray("res://addons/custom_polygon_tool/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
